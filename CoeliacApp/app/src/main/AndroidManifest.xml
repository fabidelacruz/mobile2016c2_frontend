<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ar.edu.utn.frba.coeliacs.coeliacapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".models.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".models.MainActivity" />
        <activity android:name=".models.MapActivity" />
        <activity android:name=".models.CodeBarReaderActivity" />
        <activity android:name=".models.SearchActivity" />
        <activity android:name=".models.DiscountsActivity"></activity>
    </application>

</manifest>
